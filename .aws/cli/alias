[toplevel]

region = aws configure get region
list-regions = !aws ec2 describe-regions --query 'Regions[*].RegionName' --output text | tr '\t' '\n'
list-ec2 = ec2 describe-instances \
  --query 'Reservations[].Instances[].{ID:InstanceId,Name:Tags[?Key=="Name"].Value|[0],State:State.Name}' \
  --output text
